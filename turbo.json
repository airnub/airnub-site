{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "pnpm-lock.yaml"
  ],
  "globalEnv": [
    "NEXT_PUBLIC_SITE_URL",
    "NEXT_PUBLIC_DOCS_URL"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "dist/**",
        "!.next/cache/**"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "a11y": {
      "outputs": []
    },
    "links": {
      "outputs": []
    },
    "sbom": {
      "outputs": [
        "sbom-*.json"
      ]
    },
    "i18n:sync": {
      "outputs": []
    }
  }
}